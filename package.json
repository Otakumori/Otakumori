{
  "name": "otakumori",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:scripts": "tsc --project tsconfig.json --outDir dist --rootDir . lib/*.ts scripts/*.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update-products": "node scripts/updatePrintifyProducts.js",
    "cron": "node scripts/cron.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "is-ci || husky install",
    "db:migrate": "node scripts/apply-migrations.js",
    "db:migrate:cli": "node scripts/apply-migrations-cli.js",
    "db:verify": "node scripts/verify-database.js",
    "db:seed": "prisma db seed",
    "db:seed:shop": "npx tsx scripts/seed-shop.ts",
    "db:maintain": "node scripts/maintain-database.js",
    "seed": "prisma db seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "doctor": "npm run type-check && npm run build",
    "analyze": "ANALYZE=true next build",
    "logs:view": "node scripts/view-logs.js",
    "logs:clear": "node scripts/clear-logs.js",
    "system:check": "node scripts/check-system.js",
    "system:monitor": "node scripts/monitor-system.js",
    "system:report": "node scripts/generate-health-report.js",
    "maintenance:schedule": "node scripts/schedule-maintenance.js",
    "logs:rotate": "node scripts/rotate-logs.js",
    "metrics:start": "node scripts/start-metrics-collector.js",
    "metrics:dev": "ts-node scripts/collect-metrics.ts",
    "cron:logs": "node scripts/cron-logs.js",
    "health:start": "node scripts/start-health-check.js",
    "health:dev": "ts-node scripts/check-health.ts",
    "monitoring:start": "node scripts/start-monitoring.js",
    "inngest:dev": "npx inngest-cli@latest dev",
    "inngest:deploy": "npx inngest-cli@latest deploy",
    "inngest:logs": "npx inngest-cli@latest logs",
    "test:webhooks": "curl -X GET http://localhost:3000/api/webhooks/inngest",
    "test:inngest": "curl -X POST http://localhost:8288/api/inngest -H 'Content-Type: application/json' -d '{\"name\": \"test/function\", \"data\": {\"test\": true}}'",
    "test:admin": "node scripts/test-admin-setup.js",
    "assets:validate": "tsx scripts/validators/assets-validate.ts",
    "assets:sync": "tsx scripts/assets.ts",
    "gen:asset": "tsx scripts/generate.ts",
    "gen:sfx": "tsx scripts/generate-sfx.ts",
    "gen:pack": "tsx scripts/gen-pack.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.31.8",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-spring/web": "^10.0.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.17.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/blob": "^1.1.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.23.12",
    "inngest": "^3.40.1",
    "lucide-react": "^0.540.0",
    "mitt": "^3.0.1",
    "next": "14.2.30",
    "pixi.js": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.5.0",
    "react-hot-toast": "^2.6.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-tsparticles": "^2.12.2",
    "remark-gfm": "^4.0.1",
    "resend": "^6.0.1",
    "seedrandom": "^3.0.5",
    "stripe": "^14.12.0",
    "svix": "^1.73.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "three": "^0.179.1",
    "tsparticles": "^3.9.1",
    "zod": "^3.22.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
