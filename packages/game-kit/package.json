{
  "name": "@om/game-kit",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean --external @dimforge/rapier3d-compat --external @om/ecs --external react --external three",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --max-warnings=0",
    "test": "vitest run"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.14.0"
  },
  "peerDependencies": {
    "@om/ecs": "workspace:*",
    "react": "^18.2.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@om/ecs": "workspace:*",
    "@types/react": "^18.3.26",
    "@types/three": "^0.180.0",
    "react": "^18.2.0",
    "three": "^0.180.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
